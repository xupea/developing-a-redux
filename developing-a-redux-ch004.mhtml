<code class=3D"o">=3D</code> <code class=3D"nx">actions</code><code class=
=3D"p">.</code><code class=3D"nx">openNote</code><code class=3D"p">(</code>=
<code class=3D"s1">'id-123'</code><code class=3D"p">);</code>
    <code class=3D"kr">const</code> <code class=3D"nx">expectedAction</code=
> <code class=3D"o">=3D</code> <code class=3D"p">{</code>
      <code class=3D"nx">type</code><code class=3D"o">:</code> <code class=
=3D"s1">'app/openNote'</code><code class=3D"p">,</code>
      <code class=3D"nx">payload</code><code class=3D"o">:</code> <code cla=
ss=3D"p">{</code>
        <code class=3D"nx">id</code><code class=3D"o">:</code> <code class=
=3D"s1">'id-123'</code><code class=3D"p">,</code>
      <code class=3D"p">},</code>
